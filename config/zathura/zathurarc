# zathurarc-dark
set default-bg                  \#151515
set default-fg                  \#202020
set statusbar-fg                \#B0B0B0
set statusbar-bg                \#202020
set inputbar-bg                 \#151515
set inputbar-fg                 \#808080
set notification-error-bg       \#AC4142
set notification-error-fg       \#151515
set notification-warning-bg     \#AC4142
set notification-warning-fg     \#151515
set highlight-color             \#F4BF75
set highlight-active-color      \#6A9FB5
set completion-highlight-fg     \#303030
set completion-highlight-bg     \#75B5AA
set completion-bg               \#303030
set completion-fg               \#75B5AA
set notification-bg             \#90A959
set notification-fg             \#151515
set recolor-lightcolor          \#fafafa
set recolor-darkcolor           \#212121
set window-height               675
map [fullscreen] ,              navigate previous
map [normal]     ,              navigate previous
map [fullscreen] .              navigate next
map [normal]     .              navigate next
map [fullscreen] b              scroll full-up
map [normal]     b              scroll full-up
map [fullscreen] u              scroll half-up
map [normal]     u              scroll half-up
map [fullscreen] d              scroll half-down
map [normal]     d              scroll half-down
map [fullscreen] <BackSpace>    scroll half-up
map [normal]     <BackSpace>    scroll half-up
map [fullscreen] <Return>       scroll half-down
map [normal]     <Return>       scroll half-down
